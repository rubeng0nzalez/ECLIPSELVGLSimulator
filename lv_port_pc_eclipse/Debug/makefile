# Tool invocations
lv_sim_eclipse_sdl: $(OBJS) $(USER_OBJS) makefile $(OPTIONAL_TOOL_DEPS)
	@echo 'Building target: $@'
	@echo 'Invoking: Cross GCC Linker'
	gcc -fsanitize=address -fsanitize=undefined -fno-omit-frame-pointer -o "lv_sim_eclipse_sdl" $(OBJS) $(USER_OBJS) $(LIBS) -lSDL2main -lm -lSDL2
	@echo 'Finished building target: $@'
	@echo ' '

